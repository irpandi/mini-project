{
  "extends": [
    "config:base"
  ],
  "automerge": true,
  "automergeType": "branch",
  "automergeStrategy": "green",
  "baseBranch": "master",
  "branchPrefix": "renovate/",
  "labels": ["renovate"],
  "commitMessagePrefix": "fix: ",
  "prHourlyLimit": 0,
  "prConcurrentLimit": 0,
  "ignorePresets": ["config:base"],
  "packageRules": [
    {
      "updateTypes": ["major", "minor", "patch"],
      "automerge": true
    }
  ]
}