{
  "extends": [
    "config:base"
  ],
  "automerge": true,
  "automergeType": "branch",
  "baseBranch": "master",
  "commitMessagePrefix": "fix: ",
  "prHourlyLimit": 0,
  "prConcurrentLimit": 0,
  "packageRules": [
    {
      "updateTypes": ["major", "minor", "patch"],
      "automerge": true
    }
  ],
  "enabled": true,
  "ignorePresets": ["config:base"],
  "recreateClosed": true,
  "automergeStrategy": "all",
  "prCreation": "immediate"
}
